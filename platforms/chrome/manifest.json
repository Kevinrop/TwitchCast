{
    "manifest_version": 2,
    "name": "TwitchCast",
    "description": "Unofficial Twitch mobile app",
    "version": "0.4.3",
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "clipboardWrite",
        "clipboardRead",
        "unlimitedStorage",
        "https:\/\/api.twitch.tv"
    ],
    "short_name": "TwitchCast",
    "offline_enabled": false,
    "optional_permissions": [

    ],
    "externally_connectable": {
        "ids": [

        ],
        "matches": [

        ],
        "accepts_tls_channel_id": false
    }
}